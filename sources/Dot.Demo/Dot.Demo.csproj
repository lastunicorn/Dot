    <AssemblyName>DustInTheWind.Dot.Demo</AssemblyName>
    <RootNamespace>DustInTheWind.Dot.Demo</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Ascii\acorn.ascii" />
